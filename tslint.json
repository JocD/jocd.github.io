{
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": [true, ".prettierrc"],
    "jsx-no-lambda": false,
    "import-name": false,
    "no-boolean-literal-compare": false,
    "no-empty-interface": false,
    "no-submodule-imports": false
  }
}